{
  "name": "killa",
  "version": "0.1.1",
  "description": "State management for Vanillajs",
  "main": "./cjs/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./esm/index.js"
      },
      "module": "./esm/index.js",
      "default": "./cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run prebuild && node esbuild.config.js",
    "prebuild": "shx rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "keywords": [
    "killa",
    "state",
    "management",
    "store"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesuhrz/killa.git"
  },
  "author": "Jesus Hernandez <jesusbeckan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jesuhrz/killa/issues"
  },
  "homepage": "https://github.com/jesuhrz/killa#readme",
  "dependencies": {
    "clone": "2.1.2"
  },
  "devDependencies": {
    "esbuild": "^0.17.5",
    "esbuild-plugin-alias-path": "^2.0.2",
    "node-glob": "^1.2.0",
    "shx": "^0.3.4",
    "standard": "^17.0.0"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json"
    ]
  }
}
